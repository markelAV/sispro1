<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="app.Model.dao.Site" table="web_site" catalog="laba_sispro">
        <id name="name" type="java.lang.Integer">
            <column name="id"  not-null="true"/>
            <generator class="identity" />
        </id>
        <property name="name" type="string">
            <column name="name_link" length="100"  not-null="true"/>
        </property>
        <property name="type" type="string">
            <column name="type_access" length="45" />
        </property>
        <property name="data" type="string">
            <column name="data" length="15"  unique="true" />
        </property>
    </class>
</hibernate-mapping>